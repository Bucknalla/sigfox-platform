{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "free": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "GET",
        "url": "https://smsapi.free-mobile.fr/sendmsg",
        "query": {
          "user": "{user}",
          "pass": "{pass}",
          "msg": "{msg}"
        }
      },
      "functions": {
        "sendSMS": ["user", "pass", "msg"]
      }
    }]
  }
}
