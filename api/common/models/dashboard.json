{
  "name": "Dashboard",
  "plural": "Dashboards",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "FireLoop": true,
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for Asset registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "options": {
      "type": "object"
    },
    "widgets": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "user": {
      "type": "belongsTo",
      "model": "user"
    }
  }
}
