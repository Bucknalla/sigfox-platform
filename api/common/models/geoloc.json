{
  "name": "Geoloc",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true
    },
    "lat": {
      "type": "number",
      "required": true
    },
    "lng": {
      "type": "number",
      "required": true
    },
    "precision": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {},
  "mixins": {
    "FireLoop": true,
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for Geoloc registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  }
}
